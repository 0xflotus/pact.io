{
  "name": "@pact-foundation/pact.io",
  "version": "1.0.0",
  "description": "The pact.io website where we explain what pact is and try to achieve",
  "homepage": "https://github.com/pact-foundation/pact.io#readme",
  "engine": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pact-foundation/pact.io.git"
  },
  "keywords": [
    "pact",
    "foundation",
    "pact.io",
    "pact-foundation",
    "website"
  ],
  "author": "Michel Boudreau <michelboudreau@gmail.com> (codinghitchhiker.com)",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pact-foundation/pact.io/issues"
  },
  "main": "n/a",
  "dependencies": {
    "axios": "^0.17.1",
    "gsap": "^1.20.3",
    "nuxt": "^1.0.0-rc11",
    "nuxt-class-component": "^1.1.2",
    "tachyons": "^4.9.0",
    "vue-class-component": "^6.1.0",
    "vue-property-decorator": "^6.0.0",
    "vuex-class": "^0.3.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.53",
    "ts-loader": "^3.1.1",
    "typescript": "^2.6.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "rimraf": "^2.6.2",
    "write-good": "^0.11.3",
    "remark-cli": "^4.0.0",
    "remark-preset-lint-recommended": "^3.0.1"
  },
  "scripts": {
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "clean": "rimraf -rf dist",
    "watch": "nuxt",
    "lint": "npm run lint:src && npm run lint:md && npm run lint:english",
    "lint:src": "tslint -p tsconfig.json",
    "lint:md": "remark content/posts/",
    "lint:english": "write-good './content/**/*.md'"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
