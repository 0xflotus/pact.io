{
  "name": "@pact-foundation/pact.io",
  "version": "1.0.0",
  "description": "The pact.io website where we explain what pact is and try to achieve",
  "homepage": "https://github.com/pact-foundation/pact.io#readme",
  "engine": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pact-foundation/pact.io.git"
  },
  "keywords": [
    "pact",
    "foundation",
    "pact.io",
    "pact-foundation",
    "website"
  ],
  "author": "Michel Boudreau <michelboudreau@gmail.com> (codinghitchhiker.com)",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pact-foundation/pact.io/issues"
  },
  "main": "n/a",
  "dependencies": {
    "@fortawesome/fontawesome-free-webfonts": "1.0.9",
    "@pact-foundation/pact-logo": "1.4.1",
    "babel-eslint": "7.2.1",
    "bootstrap": "4.1.1",
    "change-case": "3.0.2",
    "codeclimate-test-reporter": "0.5.0",
    "gatsby": "1.9.273",
    "gatsby-link": "1.6.45",
    "gatsby-plugin-glamor": "1.6.13",
    "gatsby-plugin-google-analytics": "1.0.31",
    "gatsby-plugin-manifest": "1.0.27",
    "gatsby-plugin-offline": "1.0.18",
    "gatsby-plugin-sass": "1.0.26",
    "gatsby-plugin-sharp": "1.6.48",
    "gatsby-plugin-typescript": "1.4.20",
    "gatsby-remark-autolink-headers": "1.4.19",
    "gatsby-remark-copy-linked-files": "1.5.37",
    "gatsby-remark-images": "1.5.67",
    "gatsby-remark-prismjs": "1.2.11",
    "gatsby-source-filesystem": "1.5.39",
    "gatsby-transformer-json": "1.0.20",
    "gatsby-transformer-remark": "1.7.44",
    "gatsby-transformer-sharp": "1.6.27",
    "graphql-code-generator": "0.8.14",
    "gray-matter": "2.1.1",
    "lodash": "4.17.4",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "react-particles-js": "2.2.0",
    "react-redux": "5.0.6",
    "reactstrap": "5.0.0-alpha.4",
    "redux-devtools-extension": "2.13.2",
    "semantic-ui-react": "0.77.1",
    "slash": "1.0.0",
    "startbootstrap-agency": "4.0.0-beta",
    "ts-loader": "3.2.0",
    "typescript": "2.6.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.3.3",
    "@storybook/addon-knobs": "3.3.3",
    "@storybook/addon-notes": "3.3.3",
    "@storybook/addon-options": "3.3.3",
    "@storybook/react": "3.3.3",
    "@types/enzyme": "3.1.6",
    "@types/jest": "21.1.10",
    "@types/lodash": "4.14.91",
    "@types/node": "8.5.2",
    "@types/react": "16.0.27",
    "@types/react-dom": "16.0.3",
    "@types/react-helmet": "5.0.3",
    "@types/react-redux": "5.0.14",
    "@types/react-router": "4.0.20",
    "@types/react-router-dom": "4.2.3",
    "@types/react-test-renderer": "16.0.0",
    "@types/reactstrap": "5.0.6",
    "@types/storybook__addon-actions": "3.0.1",
    "@types/storybook__addon-knobs": "3.2.1",
    "@types/storybook__addon-notes": "3.0.1",
    "@types/storybook__addon-options": "3.2.0",
    "@types/storybook__react": "3.0.5",
    "@types/webpack-env": "1.13.3",
    "babel-loader": "7.1.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint-plugin-react": "6.10.3",
    "flat": "2.0.1",
    "gatsby-plugin-favicon": "2.1.1",
    "jest": "19.0.2",
    "lint-staged": "3.4.0",
    "mkdirp": "0.5.1",
    "plop": "1.9.1",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16",
    "remark-cli": "3.0.1",
    "remark-preset-lint-recommended": "2.0.0",
    "storybook-readme": "3.1.1",
    "stylelint": "7.10.1",
    "stylelint-config-standard": "16.0.0",
    "ts-jest": "19.0.8",
    "tslint": "5.10.0"
  },
  "scripts": {
    "clean": "rimraf -rf public .cache",
    "start": "npm run develop",
    "watch": "npm run develop",
    "develop": "node_modules/.bin/gatsby develop -H 0.0.0.0",
    "serve": "node_modules/.bin/gatsby serve",
    "build": "node_modules/.bin/gatsby build",
    "build:pp": "node_modules/.bin/gatsby build --prefix-paths",
    "lint": "npm run lint:ts && npm run lint:css && npm run lint:md",
    "lint:ts": "tslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "lint:md": "remark .",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:scss": "stylelint src/**/*.scss --syntax scss",
    "test": "npm run lint && jest",
    "test:watch": "jest --watch",
    "generate": "plop --plopfile ./generators/plopfile.js",
    "graphql-types": "gql-gen --url http://localhost:8000/___graphql --template typescript --out ./src/graphql-types.d.ts",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o public/docs",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "jest": {
    "globals": {
      "__LINK_PREFIX__": "",
      "__PREFIX_LINKS__": ""
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/templates/",
      "/.cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  }
}
